block('page').elem('css')(
    def()(function() {
        var elem = this.elem,
            prefix = leStatics.getUrl(leBundles.getBundlePath(this.data.bundleName) + '/' + this.data.bundleName + '.min');

        return [
            { elem : elem, url : prefix + '.css', ie : false },
            { elem : elem, url : prefix + '.ie.css', ie : 'lte IE 9' }
        ];
    })
);
